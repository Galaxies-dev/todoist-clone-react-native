{
  "expo": {
    "name": "todoistClone",
    "slug": "todoistClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "todoistclone",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.supersimon.todoistClone",
      "icon": {
        "light": "./assets/images/icon.png",
        "dark": "./assets/images/icon-dark.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router",
      "react-native-bottom-tabs",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "todoist",
          "organization": "galaxies"
        }
      ],
      [
        "expo-dynamic-app-icon",
        {
          "default": {
            "image": "./assets/images/icon.png",
            "prerendered": true
          },
          "dark": {
            "image": "./assets/images/icon-dark.png",
            "prerendered": true
          },
          "green": {
            "image": "./assets/images/icon-green.png",
            "prerendered": true
          },
          "blue": {
            "image": "./assets/images/icon-blue.png",
            "prerendered": true
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "5b258ea1-151d-415b-92fc-4411162ce6b9"
      }
    }
  }
}
